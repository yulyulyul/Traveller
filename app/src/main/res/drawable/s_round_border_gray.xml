<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape
            android:shape="rectangle">

            <corners android:radius="10dp" />

            <stroke
                android:width="1dp"
                android:color="@color/non_clicked" />

            <solid
                android:color="#ffffff" />


        </shape>
    </item>
</layer-list>